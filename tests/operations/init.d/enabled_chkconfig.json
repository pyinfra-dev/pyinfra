{
    "args": ["nginx"],
    "kwargs": {
        "enabled": true
    },
    "facts": {
        "initd_status": {
            "nginx": true
        },
        "find_links": {
            "/etc/rc*.d/S*nginx": []
        },
        "linux_distribution": {
            "name": "CentOS"
        }
    },
    "commands": [
        "chkconfig nginx --add",
        "chkconfig nginx on"
    ],
    "idempotent": false
}
