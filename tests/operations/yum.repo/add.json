{
    "args": ["somerepo"],
    "kwargs": {
        "baseurl": "http://baseurl",
        "description": "description",
        "gpgcheck": false,
        "gpgkey": "test"
    },
    "facts": {
        "file": {
            "/etc/yum.repos.d/somerepo.repo": {}
        }
    },
    "commands": [[
        "upload",
        "[somerepo]\nname=description\nbaseurl=http://baseurl\nenabled=1\ngpgcheck=0\ngpgkey=test\n",
            "/etc/yum.repos.d/somerepo.repo"
    ]]
}
