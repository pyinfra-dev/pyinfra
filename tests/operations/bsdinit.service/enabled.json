{
    "args": ["nginx"],
    "kwargs": {
        "enabled": true
    },
    "facts": {
        "server.Os": "FreeBSD",
        "bsdinit.RcdStatus": {
            "nginx": true
        },
        "files.FindInFile": {
            "path=/etc/rc.conf.local, pattern=^nginx_enable=.*$, interpolate_variables=False": [],
            "path=/etc/rc.conf.local, pattern=^.*nginx_enable=\"YES\".*$, interpolate_variables=False": []
        }
    },
    "commands": [
        "echo 'nginx_enable=\"YES\"' >> /etc/rc.conf.local"
    ],
    "idempotent": false
}
