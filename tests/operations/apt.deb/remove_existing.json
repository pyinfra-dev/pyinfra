{
    "args": ["http://somewhere.com/something.deb"],
    "kwargs": {
        "present": false
    },
    "facts": {
        "files.File": {
            "path=_tempfile_": ""
        },
        "deb.DebPackage": {
            "name=_tempfile_": {
                "name": "test",
                "version": 0
            }
        },
        "deb.DebPackages": {
            "test": [0]
        }
    },
    "commands": [
        "DEBIAN_FRONTEND=noninteractive apt-get -y -o Dpkg::Options::=\"--force-confdef\" -o Dpkg::Options::=\"--force-confold\" remove test"
    ]
}
