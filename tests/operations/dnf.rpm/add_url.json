{
    "args": ["https://something.rpm"],
    "facts": {
        "file": {
            "_tempfile_.rpm": null
        },
        "rpm_package": {
            "_tempfile_.rpm": null
        },
        "which": {
            "curl": "yes"
        }
    },
    "commands": [
        "curl -sSLf 'https://something.rpm' -o _tempfile_.rpm",
        "rpm -q `rpm -qp _tempfile_.rpm` 2> /dev/null || rpm -i _tempfile_.rpm"
    ],
    "idempotent": false
}
